    <validationRules>
        <fullName>HEP_Retry_Fields_Required</fullName>
        <active>true</active>
        <description>Validates if retry fields are populated in case retry flag is set as true</description>
        <errorConditionFormula>(Retry_Flag__c = TRUE) &amp;&amp; (ISBLANK( Retry_Interval__c) || ISBLANK(  Retry_Limit__c ))</errorConditionFormula>
        <errorMessage>Please set retry interval and retry limit for interface where retry is allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HEP_Unique_Record_Validation</fullName>
        <active>false</active>
        <description>Validates if same integration record is inserted again or not</description>
        <errorConditionFormula>and(
Name = VLOOKUP
($ObjectType.HEP_Interface__c.Fields.Name , $ObjectType.HEP_Interface__c.Fields.Name, Name ),

not(
Id=VLOOKUP
($ObjectType.HEP_Interface__c.Fields.Id , $ObjectType.HEP_Interface__c.Fields.Name, Name )))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>The new record violates uniqueness constraint on the record</errorMessage>
    </validationRules>
